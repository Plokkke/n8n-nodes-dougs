{
  "name": "@plokkke/n8n-nodes-dougs",
  "version": "0.1.6",
  "description": "Nodes for n8n to interact with Dougs API",
  "keywords": [
    "n8n-community-node-package",
    "dougs"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Antoine Caron",
    "email": "antoine@crn-tech.fr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Plokkke/n8n-nodes-dougs.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DougsLoginApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Dougs/Dougs.node.js"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.14",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.70.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@plokkke/dougs-compta": "^0.4.1",
    "axios": "^1.7.9",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "zod": "^3.24.1"
  }
}
